{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "labName": {
      "value": "$(labName)"
    },
    "vhdFileUris": {
      "value": "$(vhdFileUris)"
    },
    "imageOsType": {
      "value": "Windows"
    },
    "isVhdSysPrepped": {
      "value": true
    },
    "customImageNames": {
      "value": "$(customImageNames)"
    },
    "imageDescription": {
      "value": "$(labImageDescription)"
    }
  }
}
